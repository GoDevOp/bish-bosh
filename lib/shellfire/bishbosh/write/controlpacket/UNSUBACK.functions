bishbosh_write_controlpacket_UNSUBACK()
{
	local packetIdentifier
	bishbosh_write_controlpacket_packetIdentifierCheck bishbosh_write_controlpacket_UNSUBACK_packetIdentifier "$bishbosh_write_controlpacket_UNSUBACK_packetIdentifier"
	
	# 11 << 4 in octal, followed by 2 (remaining length)
	printf '\260\002'
	
	bishbosh_write_twoByteLength $packetIdentifier
}