core_usesIn bishbosh/connection write
bishbosh_connection_write_PUBREC()
{
	local packetIdentifier
	bishbosh_connection_write_packetIdentifierCheck bishbosh_connection_write_PUBREC_packetIdentifier "$bishbosh_connection_write_PUBREC_packetIdentifier"
	
	# 5 << 4 in octal, followed by 2 (remaining length)
	printf '\120\002'
	bishbosh_connection_write_twoByteLength $packetIdentifier
	
	unset bishbosh_connection_write_PUBCOMP_packetIdentifier
	
	bishbosh_connection_ping_recordLastSentControlPacketAt
}
