function dependency_check_Fedora_checkFunction()
{
	if [ -f /etc/fedora-release ]; then
		readonly distributionName="Fedora"
		return 0
	fi
	return 1
}
dependency_check_register dependency_check_Fedora_checkFunction

function dependency_check_Fedora_packageManager()
{
	dependency_check_CentOS_packageManager
}
