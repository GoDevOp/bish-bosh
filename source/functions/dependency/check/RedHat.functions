function dependency_check_RedHat_checkFunction()
{
	if [ -f /etc/redhat-release ]; then
		readonly distributionName="RedHat"
		return 0
	fi
	return 1
}
dependency_check_register dependency_check_RedHat_checkFunction

function dependency_check_RedHat_packageManager()
{
	dependency_check_CentOS_packageManager
}
